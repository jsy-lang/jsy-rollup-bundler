{
  "name": "jsy-rollup-bundler",
  "version": "0.5.0",
  "description": "JSY-oriented rollup bundling build chain",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "license": "BSD-2-Clause",
  "keywords": [
    "JSY",
    "syntax",
    "offside",
    "python",
    "indention",
    "indent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanewholloway/jsy-rollup-bundler"
  },
  "bugs": {
    "url": "https://github.com/shanewholloway/jsy-rollup-bundler/issues"
  },
  "homepage": "https://github.com/shanewholloway/jsy-rollup-bundler#readme",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "files": [
    "cjs",
    "esm",
    "umd"
  ],
  "peerDependencies": {
    "chokidar": "^2.0.2",
    "rollup": "^0.56.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-jsy": "^0.10.1",
    "chokidar": "^2.0.2",
    "rollup": "^0.56.3",
    "rollup-plugin-jsy-babel": "^1.0.1"
  },
  "babel": {
    "presets": "jsy/lean"
  },
  "scripts": {
    "clean": "rm -rf cjs/* esm/*",
